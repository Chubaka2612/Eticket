{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    },
    "Console": {
      "IncludeScopes": true,
      "DisableColors": true,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss.fff | "
    }
  },
  "Caching": {
    "SlidingExpirationTimeSpan": "0.00:02:00"
  },
  "CorsOptions": {
    "AllowedOrigins": []
  },

  "ConnectionStrings": {
    "ETicketDb": "",
    "ETicketDbProd": "",
    "Redis": ""
  },
  "AllowedHosts": "*",
  "ServiceBus": {
    "ConnectionString": "",
    "QueueName": "notificationsqueue",
    "Namespace": "eticketsns.servicebus.windows.net"
  }
}
